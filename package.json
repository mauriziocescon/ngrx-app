    "@ng-bootstrap/ng-bootstrap": "3.3.0",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/entity": "^6.0.1",
    "@ngrx/router-store": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "@ngx-translate/core": "10.0.2",
    "@ngx-translate/http-loader": "3.0.1",
    "bootstrap": "4.1.3",
    "core-js": "^2.5.4",
    "font-awesome": "4.7.0",
    "ngrx-store-freeze": "0.2.4",
    "ngx-infinite-scroll": "6.0.1",
    "ngx-logger": "3.3.3",
