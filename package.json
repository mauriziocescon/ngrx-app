{
  "name": "ngrx-app",
  "version": "1.0.0",
  "description": "A demo application built on angular 6.x and ngrx (beta)",
  "author": "Maurizio Cescon <maurizio.cescon@gmail.com> (http://www.mauriziocescon.com)",
  "homepage": "http://www.mauriziocescon.com",
  "private": false,
  "engines": {
    "node": "=10.16.3",
    "npm": "=6.9.0"
  },
  "scripts": {
    "postinstall": "node ./mock-server/db/db-creator.js | npm run build",
    "start": "npm run json-server",
    "json-server": "node ./mock-server/server.js",
    "json-server-watch": "nodemon --ext js --watch ./mock-server ./mock-server/server.js",
    "ng": "ng",
    "serve": "ng serve --open | npm run json-server-watch",
    "build": "ng build --prod --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-analyzer": "webpack-bundle-analyzer ./dist/ngrx-app/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriziocescon/ngrx-app"
  },
  "keywords": [
    "angular",
    "bootstrap",
    "cescon",
    "es2017",
    "json-server",
    "maurizio",
    "mock-server",
    "ngrx",
    "sass",
    "travis",
    "typescript",
    "webpack"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/brentertz/scapegoat/blob/master/LICENSE-MIT"
    }
  ],
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
