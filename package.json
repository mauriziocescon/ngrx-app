    "@ngrx/effects": "^8.6.0",
    "@ngrx/entity": "^8.6.0",
    "@ngrx/router-store": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@ngrx/store-devtools": "^8.6.0",
    "@ngx-translate/core": "12.1.1",
    "@ngx-translate/http-loader": "4.0.0",
    "bootstrap": "4.4.1",
    "font-awesome": "4.7.0",
    "ngrx-store-freeze": "0.2.4",
    "ngx-infinite-scroll": "8.0.1",
    "ngx-logger": "4.1.2",
