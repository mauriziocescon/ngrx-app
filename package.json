    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/parse-link-header": "1.0.0",
    "codelyzer": "^5.1.2",
    "cross-env": "7.0.0",
    "faker": "4.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "0.16.1",
    "karma": "~4.3.0",
