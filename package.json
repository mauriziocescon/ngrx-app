{
  "name": "ngrx-app",
  "version": "1.0.0",
  "description": "A demo application built on angular 11.x and ngrx",
  "author": "Maurizio Cescon <maurizio.cescon@gmail.com> (http://www.mauriziocescon.com)",
  "homepage": "http://www.mauriziocescon.com",
  "private": false,
  "engines": {
    "node": ">10.16.3",
    "npm": ">6.9.0"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "npm run json-server",
    "json-server": "node ./mock-server/server.js",
    "json-server-watch": "nodemon --ext js --watch ./mock-server ./mock-server/server.js",
    "ng": "ng",
    "serve": "ng serve --open | npm run json-server-watch",
    "build": "ng build --stats-json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "bundle-analyzer": "webpack-bundle-analyzer ./dist/ngrx-app/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriziocescon/ngrx-app"
  },
  "keywords": [
    "angular",
    "bootstrap",
    "cescon",
    "es2017",
    "json-server",
    "maurizio",
    "mock-server",
    "ngrx",
    "sass",
    "travis",
    "typescript",
    "webpack"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/brentertz/scapegoat/blob/master/LICENSE-MIT"
    }
  ],
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.12",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/localize": "~16.2.0",
    "@angular/material": "^16.2.12",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@angular/service-worker": "~16.2.0",
    "@fortawesome/fontawesome-free": "6.4.2",
    "@ngrx/effects": "~16.3.0",
    "@ngrx/entity": "~16.3.0",
    "@ngrx/router-store": "~16.3.0",
    "@ngrx/store": "~16.3.0",
    "@ngrx/store-devtools": "~16.3.0",
    "@ngx-translate/core": "15.0.0",
    "@ngx-translate/http-loader": "8.0.0",
    "ngrx-store-freeze": "0.2.4",
    "ngx-infinite-scroll": "16.0.0",
    "ngx-logger": "5.0.12",
    "parse-link-header": "^2.0.0",
    "querystring": "^0.2.1",
    "roboto-fontface": "0.10.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "url": "0.11.3",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.7",
    "@angular/cli": "^16.2.7",
    "@angular/compiler-cli": "^16.2.0",
    "@types/faker": "6.6.9",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^20.0.0",
    "@types/parse-link-header": "2.0.2",
    "cross-env": "7.0.3",
    "faker": "5.5.3",
    "jasmine-core": "~4.6.0",
    "json-server": "^0.17.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nodemon": "^3.0.0",
    "typescript": "~5.1.3",
    "webpack-bundle-analyzer": "^4.0.0"
  }
}
