{
  "name": "ngrx-app",
  "version": "1.0.0",
  "description": "A demo application built on angular 11.x and ngrx",
  "author": "Maurizio Cescon <maurizio.cescon@gmail.com> (http://www.mauriziocescon.com)",
  "homepage": "http://www.mauriziocescon.com",
  "private": false,
  "engines": {
    "node": ">10.16.3",
    "npm": ">6.9.0"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "npm run json-server",
    "json-server": "node ./mock-server/server.js",
    "json-server-watch": "nodemon --ext js --watch ./mock-server ./mock-server/server.js",
    "ng": "ng",
    "serve": "ng serve --open | npm run json-server-watch",
    "build": "ng lint && ng build --prod --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-analyzer": "webpack-bundle-analyzer ./dist/ngrx-app/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriziocescon/ngrx-app"
  },
  "keywords": [
    "angular",
    "bootstrap",
    "cescon",
    "es2017",
    "json-server",
    "maurizio",
    "mock-server",
    "ngrx",
    "sass",
    "travis",
    "typescript",
    "webpack"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/brentertz/scapegoat/blob/master/LICENSE-MIT"
    }
  ],
  "dependencies": {
    "@angular/animations": "~11.0.2",
    "@angular/common": "~11.0.2",
    "@angular/compiler": "~11.0.2",
    "@angular/core": "~11.0.2",
    "@angular/forms": "~11.0.2",
    "@angular/localize": "~11.0.2",
    "@angular/platform-browser": "~11.0.2",
    "@angular/platform-browser-dynamic": "~11.0.2",
    "@angular/router": "~11.0.2",
    "@angular/service-worker": "~11.0.2",
    "@ng-bootstrap/ng-bootstrap": "9.1.3",
    "@ngrx/effects": "~11.1.0",
    "@ngrx/entity": "~11.1.0",
    "@ngrx/router-store": "~11.1.0",
    "@ngrx/store": "~11.1.0",
    "@ngrx/store-devtools": "~11.1.0",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "bootstrap": "5.0.1",
    "font-awesome": "4.7.0",
    "ngrx-store-freeze": "0.2.4",
    "ngx-infinite-scroll": "10.0.1",
    "ngx-logger": "4.2.2",
    "parse-link-header": "1.0.1",
    "roboto-fontface": "0.10.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular/cli": "~11.0.2",
    "@angular/compiler-cli": "~11.0.2",
    "@angular/language-service": "~11.0.2",
    "@types/faker": "5.5.6",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/parse-link-header": "1.0.0",
    "codelyzer": "^6.0.0",
    "cross-env": "7.0.3",
    "faker": "5.5.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "json-server": "0.16.3",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "nodemon": "2.0.7",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
