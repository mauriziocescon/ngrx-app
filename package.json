    "@types/faker": "4.1.4",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/parse-link-header": "1.0.0",
    "codelyzer": "~4.2.1",
    "cross-env": "5.2.0",
    "faker": "4.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "0.14.0",
    "karma": "~1.7.1",
