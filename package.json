{
  "name": "ngrx-app",
  "version": "1.0.0",
  "description": "A demo application built on angular 11.x and ngrx",
  "author": "Maurizio Cescon <maurizio.cescon@gmail.com> (http://www.mauriziocescon.com)",
  "homepage": "http://www.mauriziocescon.com",
  "private": false,
  "engines": {
    "node": ">10.16.3",
    "npm": ">6.9.0"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "npm run json-server",
    "json-server": "node ./mock-server/server.js",
    "json-server-watch": "nodemon --ext js --watch ./mock-server ./mock-server/server.js",
    "ng": "ng",
    "serve": "ng serve --open | npm run json-server-watch",
    "build": "ng build --stats-json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "bundle-analyzer": "webpack-bundle-analyzer ./dist/ngrx-app/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriziocescon/ngrx-app"
  },
  "keywords": [
    "angular",
    "bootstrap",
    "cescon",
    "es2017",
    "json-server",
    "maurizio",
    "mock-server",
    "ngrx",
    "sass",
    "travis",
    "typescript",
    "webpack"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/brentertz/scapegoat/blob/master/LICENSE-MIT"
    }
  ],
  "dependencies": {
    "@angular/animations": "~14.3.0",
    "@angular/common": "~14.3.0",
    "@angular/compiler": "~14.3.0",
    "@angular/core": "~14.3.0",
    "@angular/forms": "~14.3.0",
    "@angular/localize": "~14.3.0",
    "@angular/platform-browser": "~14.3.0",
    "@angular/platform-browser-dynamic": "~14.3.0",
    "@angular/router": "~14.3.0",
    "@angular/service-worker": "~14.3.0",
    "@fortawesome/fontawesome-free": "6.1.1",
    "@ng-bootstrap/ng-bootstrap": "12.1.2",
    "@ngrx/effects": "~13.2.0",
    "@ngrx/entity": "~13.2.0",
    "@ngrx/router-store": "~13.2.0",
    "@ngrx/store": "~13.2.0",
    "@ngrx/store-devtools": "~13.2.0",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "bootstrap": "5.1.3",
    "ngrx-store-freeze": "0.2.4",
    "ngx-infinite-scroll": "13.0.2",
    "ngx-logger": "5.0.11",
    "parse-link-header": "1.0.1",
    "querystring-es3": "0.2.1",
    "roboto-fontface": "0.10.0",
    "@popperjs/core": "2.11.5",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "url": "0.11.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.13",
    "@angular/cli": "~14.2.13",
    "@angular/compiler-cli": "~14.3.0",
    "@types/faker": "6.6.9",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@types/parse-link-header": "1.0.1",
    "cross-env": "7.0.3",
    "faker": "5.5.3",
    "jasmine-core": "~3.10.0",
    "json-server": "0.17.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
