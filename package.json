{
  "name": "ngrx-app",
  "version": "1.0.0",
  "description": "A demo application built on angular 11.x and ngrx",
  "author": "Maurizio Cescon <maurizio.cescon@gmail.com>",
  "homepage": "https://be.linkedin.com/in/mauriziocescon",
  "private": false,
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=10.2.3"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production npm run json-server",
    "json-server": "node ./mock-server/server.js",
    "json-server-watch": "nodemon --ext js --watch ./mock-server ./mock-server/server.js",
    "ng": "ng",
    "serve": "ng serve --open | npm run json-server-watch",
    "build": "ng build --stats-json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "bundle-analyzer": "webpack-bundle-analyzer ./dist/ngrx-app/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriziocescon/ngrx-app"
  },
  "keywords": [
    "angular",
    "bootstrap",
    "cescon",
    "es2017",
    "json-server",
    "maurizio",
    "mock-server",
    "ngrx",
    "sass",
    "travis",
    "typescript",
    "webpack"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/brentertz/scapegoat/blob/master/LICENSE-MIT"
    }
  ],
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/cdk": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/localize": "^17.0.0",
    "@angular/material": "^17.0.4",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@ngneat/transloco": "^6.0.4",
    "@ngrx/effects": "^17.0.0",
    "@ngrx/entity": "^17.0.0",
    "@ngrx/router-store": "^17.0.0",
    "@ngrx/signals": "^17.0.0",
    "@ngrx/store": "^17.0.0",
    "@ngrx/store-devtools": "^17.0.0",
    "ngx-infinite-scroll": "^17.0.0",
    "parse-link-header": "^2.0.0",
    "querystring": "^0.2.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "url": "^0.11.3",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.9",
    "@angular/cli": "^17.0.9",
    "@angular/compiler-cli": "^17.0.0",
    "@types/faker": "^6.6.9",
    "@types/jasmine": "~5.1.0",
    "@types/parse-link-header": "^2.0.3",
    "cross-env": "^7.0.3",
    "faker": "5.5.3",
    "jasmine-core": "~5.1.0",
    "json-server": "^0.17.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nodemon": "^3.0.0",
    "typescript": "~5.3.3",
    "webpack-bundle-analyzer": "^4.0.0"
  }
}
